{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": [
      "ES2021"
    ],
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    ".vscode-test",
    "out",
    "dist"
  ]
}

